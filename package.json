  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "eslint": "^5.2.0",
    "eslint-config-ktsn-typescript": "^1.1.0",
    "jest": "^23.4.1",
    "prettier": "1.13.7",
    "prettier-config-ktsn": "^1.0.0",
    "ts-jest": "^23.0.1",
    "typescript": "^2.9.2"
